datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model FitnessData {
  id       Int    @id @default(autoincrement())
  date     String
  name     String
  pushUp   Int
  plank    Int
  squat    Int
  abs      Int
}

model Users {
  id   Int    @id @default(autoincrement())
  name String
}